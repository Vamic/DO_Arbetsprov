@model IEnumerable<string>
<div class="row">
    <div class="col-sm-6 col-sm-offset-3">
        <table class="table table-bordered table-striped">
            <thead>
                <tr>
                    <th>Produkt</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < 1000; i++)
                {
                    string product = Model.ElementAt(i);
                    <tr>
                        <td>SKE @product</td>
                        <td>@Html.ActionLink("Se prishistorik", "Timeline", new { id = product }, new { @class = "btn btn-primary" })</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>
